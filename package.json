{
  "name": "vuepress-plugin-social-share",
  "type": "module",
  "version": "2.0.0-beta.7",
  "packageManager": "pnpm@7.23.0",
  "description": "Social sharing plugin for VuePress",
  "keywords": [
    "vuepress",
    "vuepress-plugin",
    "share",
    "social",
    "social-share",
    "sns",
    "facebook",
    "plugin",
    "twitter",
    "weibo"
  ],
  "homepage": "https://social-share.ntnyq.com",
  "bugs": {
    "url": "https://github.com/ntnyq/vuepress-plugin-social-share/issues"
  },
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "repository": "ntnyq/vuepress-plugin-social-share",
  "main": "./lib/node/index.js",
  "module": "./lib/node/index.js",
  "types": "./lib/node/index.d.ts",
  "exports": {
    ".": "./lib/node/index.js",
    "./client": "./lib/client/index.js",
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "run-s clean build:lib copy",
    "build:docs": "pnpm --filter=docs run build",
    "build:lib": "tsc",
    "clean": "rimraf lib *.tsbuildinfo",
    "copy": "copyfiles -a -u 1 -V src/**/*.css lib",
    "dev": "run-s clean dev:lib",
    "dev:docs": "pnpm --filter=docs run dev",
    "dev:lib": "tsc-watch --onSuccess \"npm run copy\"",
    "lint": "eslint .",
    "prepare": "husky install",
    "release": "run-s release:check release:publish",
    "release:check": "run-s lint typecheck clean build",
    "release:publish": "bumpp && pnpm publish --tag next",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vuepress": "^2.0.0-beta.51"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "qrcode": "^1.5.1"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^1.2.1",
    "@types/node": "^18.11.18",
    "@types/qrcode": "^1.5.0",
    "@vuepress/client": "^2.0.0-beta.53",
    "bumpp": "^8.2.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.31.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.23.0",
    "rimraf": "^3.0.2",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4",
    "vue": "^3.2.45",
    "vuepress": "^2.0.0-beta.53"
  },
  "nano-staged": {
    "*.{js,ts,vue,yml,yaml,md,json}": "eslint --fix"
  }
}
