{
  "name": "vuepress-plugin-social-share",
  "version": "2.0.0-beta.5",
  "packageManager": "pnpm@7.1.3",
  "description": "Social sharing plugin for VuePress",
  "keywords": [
    "vuepress",
    "vuepress-plugin",
    "share",
    "social",
    "social-share",
    "sns",
    "facebook",
    "plugin",
    "twitter",
    "weibo"
  ],
  "homepage": "https://social-share.ntnyq.com",
  "bugs": {
    "url": "https://github.com/ntnyq/vuepress-plugin-social-share/issues"
  },
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntnyq/vuepress-plugin-social-share.git"
  },
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib *.tsbuildinfo",
    "copy": "copyfiles -a -u 1 -V src/**/*.css lib",
    "dev": "run-s clean dev:lib",
    "build": "run-s clean build:lib copy",
    "dev:lib": "tsc-watch -b tsconfig.build.json --onSuccess \"npm run copy\"",
    "build:lib": "tsc --b  tsconfig.build.json",
    "dev:docs": "pnpm -C docs run dev",
    "build:docs": "pnpm -C docs run build",
    "lint": "run-p lint:*",
    "lint:cspell": "cspell \"*\"*",
    "lint:script": "eslint .",
    "prepare": "husky install",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "release": "run-s release:check release:version release:publish",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:check": "run-s lint typecheck clean build",
    "release:publish": "pnpm -r publish --tag next",
    "release:version": "bumpp package.json --execute=\"pnpm release:changelog\" --commit \"chore: release v%s\" --all --tag --push"
  },
  "peerDependencies": {
    "vuepress": "^2.0.0-beta.45"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "qrcode": "^1.5.0"
  },
  "devDependencies": {
    "@babel/types": "^7.18.0",
    "@ntnyq/eslint-config": "^1.0.2",
    "@types/node": "^17.0.35",
    "@types/qrcode": "^1.4.2",
    "@vuepress/client": "2.0.0-beta.45",
    "bumpp": "^7.1.1",
    "conventional-changelog-cli": "^2.2.2",
    "copyfiles": "^2.4.1",
    "cspell": "^6.0.0",
    "eslint": "^8.16.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.1.3",
    "rimraf": "^3.0.2",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.4",
    "vue": "^3.2.35",
    "vuepress": "2.0.0-beta.45"
  },
  "lint-staged": {
    "*.{js,ts,yaml,md,json}": "eslint --fix"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@ntnyq"
  },
  "eslintIgnore": [
    "lib"
  ]
}
