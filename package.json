{
  "name": "vuepress-plugin-social-share",
  "type": "module",
  "version": "2.0.0-beta.20",
  "packageManager": "pnpm@9.4.0",
  "description": "Social sharing plugin for VuePress",
  "keywords": [
    "vuepress",
    "vuepress-plugin",
    "share",
    "social",
    "social-share",
    "sns",
    "facebook",
    "twitter",
    "weibo"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://social-share.ntnyq.com",
  "bugs": {
    "url": "https://github.com/ntnyq/vuepress-plugin-social-share/issues"
  },
  "repository": "ntnyq/vuepress-plugin-social-share",
  "exports": {
    ".": "./dist/node/index.js",
    "./client": "./dist/client/index.js",
    "./client/*": "./dist/client/*",
    "./package.json": "./package.json"
  },
  "main": "./dist/node/index.js",
  "types": "./dist/node/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s clean build:lib copy",
    "build:docs": "pnpm -C docs run build",
    "build:lib": "tsc",
    "clean": "rimraf dist *.tsbuildinfo",
    "copy": "copyfiles -a -u 1 -V src/**/*.css dist",
    "dev": "run-s clean dev:lib",
    "dev:docs": "pnpm -C docs run dev",
    "dev:lib": "tsc-watch --onSuccess \"npm run copy\"",
    "lint": "eslint .",
    "prepare": "husky",
    "release": "run-s release:check release:publish",
    "release:check": "run-s lint typecheck clean build",
    "release:publish": "bumpp && pnpm publish --tag next",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vuepress": "^2.0.0-rc.14"
  },
  "dependencies": {
    "@types/qrcode": "^1.5.5",
    "@vuepress/helper": "^2.0.0-rc.37",
    "deepmerge": "^4.3.1",
    "qrcode": "^1.5.3",
    "vue": "^3.4.30"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^2.2.1",
    "@ntnyq/prettier-config": "^1.21.2",
    "@types/node": "^20.14.9",
    "bumpp": "^9.4.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.5.0",
    "husky": "^9.0.11",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^6.2.0",
    "pnpm": "^9.4.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.5.2",
    "vuepress": "^2.0.0-rc.14"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,vue,yml,yaml,md,json}": "eslint --fix"
  }
}
