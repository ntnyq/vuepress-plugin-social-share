{
  "name": "vuepress-plugin-social-share",
  "version": "1.1.0",
  "description": "Social sharing plugin for VuePress",
  "keywords": [
    "vuepress",
    "vuepress-plugin",
    "share",
    "social",
    "social-share",
    "sns",
    "facebook",
    "plugin",
    "twitter",
    "weibo"
  ],
  "homepage": "https://sns.goyfe.com",
  "bugs": {
    "url": "https://github.com/ntnyq/vuepress-plugin-social-share/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntnyq/vuepress-plugin-social-share.git"
  },
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "copy": "copyfiles -a -u 1 -V src/*.styl lib",
    "dev": "run-s clean dev:lib",
    "build": "run-s clean build:lib copy",
    "dev:lib": "tsc-watch --onSuccess \"npm run copy\"",
    "build:lib": "tsc build",
    "dev:playground": "npm -C playground run dev",
    "build:playground": "npm -C playground run build",
    "lint": "run-p lint:*",
    "test": "jest",
    "lint:cspell": "cspell **",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "lint-staged": {
    "*.{md,json}": "prettier --write"
  },
  "prettier": "@ntnyq/prettier-config",
  "dependencies": {
    "deepmerge": "^4.2.2",
    "qrcode": "^1.5.0"
  },
  "devDependencies": {
    "@ntnyq/prettier-config": "^0.3.0",
    "copyfiles": "^2.4.1",
    "cspell": "^5.19.5",
    "husky": "^7.0.2",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "tsc-watch": "^4.6.2",
    "typescript": "^4.6.3",
    "vue": "^2.6.14",
    "vuepress": "^1.9.7",
    "vuepress-types": "^0.9.4"
  },
  "peerDependencies": {
    "vuepress": "^1.0.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "css-loader"
      ]
    }
  }
}
