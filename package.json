{
  "name": "vuepress-plugin-social-share",
  "version": "1.3.0",
  "packageManager": "pnpm@7.23.0",
  "description": "Social sharing plugin for VuePress",
  "keywords": [
    "vuepress",
    "vuepress-plugin",
    "share",
    "social",
    "social-share",
    "sns",
    "facebook",
    "plugin",
    "twitter",
    "weibo"
  ],
  "homepage": "https://sns.goyfe.com",
  "bugs": {
    "url": "https://github.com/ntnyq/vuepress-plugin-social-share/issues"
  },
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntnyq/vuepress-plugin-social-share.git"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s clean build:lib copy",
    "build:docs": "pnpm --filter=docs run build",
    "build:lib": "tsc --build",
    "clean": "rimraf dist",
    "copy": "copyfiles -a -u 1 -V src/*.styl dist",
    "dev": "run-s clean dev:lib",
    "dev:docs": "pnpm --filter=docs run dev",
    "dev:lib": "tsc-watch --onSuccess \"npm run copy\"",
    "lint": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vuepress": "^1.0.3"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "qrcode": "^1.5.1"
  },
  "devDependencies": {
    "@ntnyq/eslint-config-typescript": "^1.2.1",
    "@types/qrcode": "^1.5.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.31.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.23.0",
    "rimraf": "^3.0.2",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4",
    "vue": "^2.7.14",
    "vuepress": "^1.9.8",
    "vuepress-types": "^0.9.4"
  },
  "nano-staged": {
    "*.{js,ts,yml,yaml,md,json}": "eslint --fix"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "css-loader"
      ]
    }
  }
}
