{
  "name": "vuepress-plugin-social-share",
  "type": "module",
  "version": "2.0.0-beta.6",
  "packageManager": "pnpm@7.13.3",
  "description": "Social sharing plugin for VuePress",
  "keywords": [
    "vuepress",
    "vuepress-plugin",
    "share",
    "social",
    "social-share",
    "sns",
    "facebook",
    "plugin",
    "twitter",
    "weibo"
  ],
  "homepage": "https://social-share.ntnyq.com",
  "bugs": {
    "url": "https://github.com/ntnyq/vuepress-plugin-social-share/issues"
  },
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntnyq/vuepress-plugin-social-share.git"
  },
  "main": "./lib/node/index.js",
  "types": "./lib/node/index.d.ts",
  "exports": {
    ".": "./lib/node/index.js",
    "./client": "./lib/client/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib *.tsbuildinfo",
    "copy": "copyfiles -a -u 1 -V src/**/*.css lib",
    "dev": "run-s clean dev:lib",
    "build": "run-s clean build:lib copy",
    "dev:lib": "tsc-watch -b tsconfig.build.json --onSuccess \"npm run copy\"",
    "build:lib": "tsc --b  tsconfig.build.json",
    "dev:docs": "pnpm --filter=docs run dev",
    "build:docs": "pnpm --filter=docs run build",
    "lint": "eslint .",
    "prepare": "husky install",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "release": "run-s release:check release:version release:publish",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:check": "run-s lint typecheck clean build",
    "release:publish": "pnpm -r publish --tag next",
    "release:version": "bumpp package.json --all --execute=\"pnpm release:changelog\""
  },
  "peerDependencies": {
    "vuepress": "^2.0.0-beta.51"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "pathe": "^0.3.9",
    "qrcode": "^1.5.1"
  },
  "devDependencies": {
    "@babel/types": "^7.19.4",
    "@ntnyq/eslint-config": "^1.0.4",
    "@types/node": "^18.8.4",
    "@types/qrcode": "^1.5.0",
    "@vuepress/client": "^2.0.0-beta.51",
    "bumpp": "^8.2.1",
    "conventional-changelog-cli": "^2.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.25.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.13.4",
    "rimraf": "^3.0.2",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.4",
    "vue": "^3.2.40",
    "vuepress": "^2.0.0-beta.51"
  },
  "lint-staged": {
    "*.{js,ts,yaml,md,json}": "eslint --fix"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@ntnyq"
  },
  "eslintIgnore": [
    "lib"
  ]
}
