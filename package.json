{
  "name": "vuepress-plugin-social-share",
  "version": "1.3.1",
  "packageManager": "pnpm@8.10.5",
  "description": "Social sharing plugin for VuePress",
  "keywords": [
    "vuepress",
    "vuepress-plugin",
    "share",
    "social",
    "social-share",
    "sns",
    "facebook",
    "plugin",
    "twitter",
    "weibo"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://social-share-v1.ntnyq.com",
  "bugs": {
    "url": "https://github.com/ntnyq/vuepress-plugin-social-share/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntnyq/vuepress-plugin-social-share.git"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s clean build:lib copy",
    "build:docs": "pnpm --filter=docs run build",
    "build:lib": "tsc --build",
    "clean": "rimraf dist",
    "copy": "copyfiles -a -u 1 -V src/*.styl dist",
    "dev": "run-s clean dev:lib",
    "dev:docs": "pnpm --filter=docs run dev",
    "dev:lib": "tsc-watch --onSuccess \"npm run copy\"",
    "lint": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vuepress": "^1.0.3"
  },
  "dependencies": {
    "deepmerge": "^4.3.1",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^1.19.0",
    "@ntnyq/prettier-config": "^1.19.0",
    "@types/qrcode": "^1.5.5",
    "copyfiles": "^2.4.1",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.10.5",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.2.2",
    "vue": "^2.7.15",
    "vuepress": "^1.9.10",
    "vuepress-types": "^0.9.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "css-loader"
      ]
    }
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,yml,yaml,md,json}": "eslint --fix"
  }
}
