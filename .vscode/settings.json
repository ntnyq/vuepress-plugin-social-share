{
  "eslint.enable": true,
  "prettier.enable": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "cSpell.words": [
    "customed",
    "goyfe",
    "linkedin",
    "ntnyq",
    "pnpm",
    "qrcode",
    "rgba",
    "serializers",
    "styl",
    "tsbuildinfo",
    "vetur",
    "vuepress",
    "weibo"
  ],
  "cSpell.allowCompoundWords": true
}
