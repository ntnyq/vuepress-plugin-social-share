{
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "json5"
  ],
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.formatOnSave": false
  },
  "cSpell.words": [
    "bumpp",
    "changelogithub",
    "customed",
    "goyfe",
    "linkedin",
    "ntnyq",
    "pnpm",
    "qrcode",
    "rgba",
    "serializers",
    "styl",
    "tsbuildinfo",
    "vetur",
    "vuepress",
    "weibo"
  ],
  "cSpell.allowCompoundWords": true
}
